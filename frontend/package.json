{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "check": "pnpm lint && pnpm format:check && pnpm typecheck",
    "ci-basic": "pnpm check && pnpm build",
    "ci-full": "pnpm check && pnpm test:coverage && pnpm build",
    "generate:tests": "qodo-cli test generate --framework=vitest --location=colocated --output-format=typescript --config-file=vitest.config.ts",
    "shadcn-ui": "pnpm dlx shadcn@latest",
    "clean": "rm -rf .next node_modules pnpm-lock.yaml",
    "reinstall": "pnpm run clean && pnpm install"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "geist": "^1.4.2",
    "lucide-react": "^0.513.0",
    "motion": "^12.16.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^6.0.0",
    "shadcn-ui": "^0.9.5",
    "tailwind-merge": "^3.3.0",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.25.55",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/coverage-v8": "^3.2.2",
    "@vitest/ui": "3.2.2",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.2"
  },
  "packageManager": "pnpm@10.12.1"
}
